  },
  "homepage": "https://github.com/ovieokeh/avalanche-dapp-tutorial#readme",
  "devDependencies": {
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "dotenv": "^10.0.0",
    "ethers": "^5.5.4",
    "hardhat": "^2.7.0",
    "sass": "^1.43.5"
  },
  "dependencies": {
    "next": "^12.0.10",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react95": "^3.11.1",
    "styled-components": "^5.3.3"
  }
